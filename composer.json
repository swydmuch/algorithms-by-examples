{
    "name": "swydmuch/algorithms-by-examples",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "AlgorithmsByExamples\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "AlgorithmsByExamples\\Tests\\" : "tests/"
        }
    },
    "authors": [
        {
            "name": "sylwekw"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^2.0",
        "infection/infection": "^0.29.10"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
